{
  "name": "seasx/seas-logger",
  "description": "An effective,fast,stable log package for PHP",
  "keywords": [
    "log",
    "logging",
    "psr-3"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "SeasX",
      "homepage": "https://github.com/SeasX"
    }
  ],
  "require": {
    "php": "^7.0",
    "psr/log": "^1.0.2",
    "ext-SeasLog": "^1.7",
    "friendsofphp/php-cs-fixer": "^2.11"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5"
  },
  "autoload": {
    "psr-4": {
      "Seasx\\SeasLogger\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Seasx\\SeasLogger\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": [
      "phpunit"
    ]
  }
}
